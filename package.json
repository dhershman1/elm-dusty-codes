{
  "name": "elm-dusty-codes",
  "version": "1.0.0",
  "description": "Just my portfolio site built using an Elm front end.",
  "main": "elm.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "build:dev": "elm-live src/Main.elm --no-server -- --output=public/assets/js/elm.js",
    "build": "elm make src/Main.elm --optimize --output=public/assets/js/elm.js",
    "dev": "run-p build:dev start:dev",
    "prod": "run-s build start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/elm-dusty-codes.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhershman1/elm-dusty-codes/issues"
  },
  "homepage": "https://github.com/dhershman1/elm-dusty-codes#readme",
  "dependencies": {
    "elm": "0.19.1-3",
    "express": "4.17.3"
  },
  "devDependencies": {
    "elm-live": "4.0.2",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5"
  }
}
